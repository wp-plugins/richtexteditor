<styles>
	<!--
	These attributes will execute the config.format_%attrname%
	
	value='any value' :
	subscript,superscript,bold,italic,underline,linethrough,overline,
	
	value='specified value' :
	forecolor,backcolor,fontsize,fontname,cssclass,cssstyle,
	-->
	<style name="@clearstyle" />
	<style name="Comment" italic="1" cssstyle="color:darkgreen;" />
	<style name="Highlight" cssstyle="background-color:yellow;" />
	<style name="attachment" cssclass="attachment" />
	<style name="category-gallery" cssclass="category-gallery" />
	<style name="commentlist" cssclass="commentlist" />
	<style name="comment-author" cssclass="comment-author" />
	<style name="comment-meta" cssclass="comment-meta" />
	<style name="entry-content" cssclass="entry-content" />
	<style name="entry-meta" cssclass="entry-meta" />
	<style name="entry-summary" cssclass="entry-summary" />
	<style name="entry-title" cssclass="entry-title" />
	<style name="page-title" cssclass="page-title" />
	<style name="page-link" cssclass="page-link" />
	<style name="widget-area" cssclass="widget-area" />
	<style name="widget-title" cssclass="widget-title" />
	<style name="wdiget-container" cssclass="wdiget-container" />
</styles>
